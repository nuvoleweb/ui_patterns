{
  "name": "drupal/ui_patterns",
  "type": "drupal-module",
  "description": "UI Patterns.",
  "keywords": ["drupal", "web", "ui"],
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Nuvole Web",
      "email": "info@nuvole.org"
    }
  ],
  "require-dev": {
    "cweagans/composer-patches": "~1.0",
    "drupal-composer/drupal-scaffold": "^2.2",
    "drupal/config_installer": "1.3.0",
    "drupal/console": "~1",
    "drush/drush": "*",
    "drupal/core": "8.5.x-dev",
    "webflo/drupal-core-require-dev": "8.5.x-dev",
    "drupal/ds": "~3",
    "drupal/field_group": "~1",
    "drupal/coffee": "~1",
    "drupal/panels": "~4",
    "drupal/features": "~3",
    "drupal/page_manager": "*",
    "drupal/paragraphs": "~1",
    "drupal/token": "~1",
    "drupal/bootstrap": "3.1",
    "nuvoleweb/drupal-behat": "~1.1.0",
    "phpro/grumphp": "0.11.4",
    "peridot-php/leo": "^1.6",
    "openeuropa/task-runner": "^0.4",
    "phpunit/phpunit": "^6.1"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "autoload": {
    "psr-4": {"Drupal\\ui_patterns\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\Tests\\ui_patterns\\": "tests/src"
    }
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
  },
  "extra": {
    "patches": {
      "drupal/core": {
        "https://www.drupal.org/project/drupal/issues/2943172": "https://www.drupal.org/files/issues/2018-04-15/2943172-kernel-test-base-2.patch"
      }
    },
    "installer-paths": {
      "build/core": ["type:drupal-core"],
      "build/modules/contrib/{$name}": ["type:drupal-module"],
      "build/profiles/contrib/{$name}": ["type:drupal-profile"],
      "build/themes/contrib/{$name}": ["type:drupal-theme"]
    }
  }
}
